<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.online.edu.mapper.UcenterMemberMapper">
    <select id="queryRegisterCountOneDay" resultType="java.lang.Integer">
        select count(*) from ucenter_member
        where DATE(gmt_create) = #{day};
    </select>
</mapper>
